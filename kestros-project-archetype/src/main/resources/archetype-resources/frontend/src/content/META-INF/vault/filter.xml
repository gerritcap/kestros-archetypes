<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">

  <filter root="/apps/${artifactId}"/>

  <filter root="/apps/${artifactId}/install" mode="update">
    <include pattern=".*${artifactId}-core-bundle.*\.jar"/>
  </filter>
  
  <filter root="/etc/ui-frameworks/${artifactId}-framework"/>
  <filter root="/etc/vendor-libraries/${artifactId}-vendor-library"/>

</workspaceFilter>
